#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/me/dev/shunueda/node-template/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.14.0_typescript@5.8.2/node_modules/commitizen/bin/node_modules:/Users/me/dev/shunueda/node-template/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.14.0_typescript@5.8.2/node_modules/commitizen/node_modules:/Users/me/dev/shunueda/node-template/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.14.0_typescript@5.8.2/node_modules:/Users/me/dev/shunueda/node-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/me/dev/shunueda/node-template/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.14.0_typescript@5.8.2/node_modules/commitizen/bin/node_modules:/Users/me/dev/shunueda/node-template/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.14.0_typescript@5.8.2/node_modules/commitizen/node_modules:/Users/me/dev/shunueda/node-template/node_modules/.pnpm/commitizen@4.3.1_@types+node@22.14.0_typescript@5.8.2/node_modules:/Users/me/dev/shunueda/node-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../commitizen/bin/commitizen" "$@"
else
  exec node  "$basedir/../commitizen/bin/commitizen" "$@"
fi
